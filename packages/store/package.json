{
  "name": "@starter/store",
  "version": "0.1.0",
  "description": "TanStack Store wrapper for fullstack-starter",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch",
    "lint": "biome check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@tanstack/react-store": "^0.5.5"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@starter/typescript": "workspace:*",
    "@types/react": "^18.3.18",
    "react": "^18.3.1",
    "tsup": "^8.5.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
