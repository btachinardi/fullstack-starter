{
  "name": "@starter/data-access",
  "version": "0.1.0",
  "description": "API client and data access for fullstack-starter",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./resources": {
      "types": "./dist/resources.d.ts",
      "import": "./dist/resources.js",
      "require": "./dist/resources.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts src/resources.ts --dts --format esm,cjs --clean",
    "dev": "tsup src/index.ts src/resources.ts --dts --format esm,cjs --watch",
    "lint": "biome check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@starter/utils": "workspace:*",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@starter/typescript": "workspace:*",
    "tsup": "^8.5.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
