{
  "name": "@starter/db",
  "version": "0.1.0",
  "description": "Prisma database wrapper for fullstack-starter",
  "type": "commonjs",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts"
  },
  "files": ["src", "dist", "prisma"],
  "scripts": {
    "build": "tsc",
    "postinstall": "prisma generate",
    "lint": "echo 'Lint via root'",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1"
  },
  "devDependencies": {
    "@starter/typescript": "workspace:*",
    "@types/node": "^22.10.2",
    "jest": "^29.7.0",
    "prisma": "^6.2.1",
    "typescript": "^5.7.2"
  }
}
