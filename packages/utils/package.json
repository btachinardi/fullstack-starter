{
    "name": "@starter/utils",
    "version": "0.1.0",
    "description": "Web utilities for fullstack-starter",
    "type": "module",
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": "./src/index.ts",
        "./errors": "./src/errors.ts",
        "./logger": "./src/logger.ts",
        "./format": "./src/format.ts"
    },
    "typesVersions": {
        "*": {
            "errors": [
                "./src/errors.ts"
            ],
            "logger": [
                "./src/logger.ts"
            ],
            "format": [
                "./src/format.ts"
            ]
        }
    },
    "files": [
        "src",
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "lint": "echo 'Lint via root'",
        "typecheck": "tsc --noEmit",
        "test": "vitest run"
    },
    "dependencies": {
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@starter/typescript": "workspace:*",
        "@types/node": "^22.10.2",
        "typescript": "^5.7.2",
        "vitest": "^2.1.8"
    }
}