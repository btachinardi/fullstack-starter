{
  "name": "@starter/api",
  "version": "0.1.0",
  "description": "NestJS utilities for fullstack-starter",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./dtos": {
      "types": "./dist/dtos/index.d.ts",
      "default": "./dist/dtos/index.js"
    }
  },
  "files": ["src", "dist"],
  "scripts": {
    "build": "tsc",
    "lint": "biome check .",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/swagger": "^8.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1"
  },
  "devDependencies": {
    "@starter/typescript": "workspace:*",
    "@types/node": "^22.10.2",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.2.2",
    "typescript": "^5.7.2"
  }
}
