{
  "name": "@starter/api",
  "version": "0.1.0",
  "description": "NestJS utilities for fullstack-starter",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dtos": {
      "types": "./dist/dtos/index.d.ts",
      "import": "./dist/dtos/index.js",
      "require": "./dist/dtos/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts src/dtos/index.ts --dts --format esm,cjs --clean",
    "dev": "tsup src/index.ts src/dtos/index.ts --dts --format esm,cjs --watch",
    "lint": "biome check .",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/swagger": "^8.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1"
  },
  "devDependencies": {
    "@starter/typescript": "workspace:*",
    "@types/node": "^22.10.2",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.2.2",
    "tsup": "^8.5.0",
    "typescript": "^5.7.2"
  }
}
