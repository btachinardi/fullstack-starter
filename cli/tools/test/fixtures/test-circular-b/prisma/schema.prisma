// ============================================================================
// Test Circular Dependency B
// ============================================================================
// This schema imports A, which imports B - should trigger circular dependency error

// @prisma-import: ../../test-circular-a/prisma/schema.prisma

generator client {
	provider = "prisma-client-js"
	output   = "../generated/prisma"
}

datasource db {
	provider = "postgresql"
	url      = env("DATABASE_URL")
	schemas  = ["circular_b"]
}

model ModelB {
	id   String @id @default(cuid())
	name String

	@@schema("circular_b")
}
