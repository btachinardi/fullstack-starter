		"axios": "^1.12.2",
		"better-auth": "^1.3.29",
		"dotenv": "^17.2.3"
	},
	"devDependencies": {
		"@configs/typescript": "workspace:*",
		"@prisma/client": "^6.18.0",
		"@types/node": "^24.9.1",
		"prisma": "^6.18.0",
		"ts-loader": "^9.5.4",
